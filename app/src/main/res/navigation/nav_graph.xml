<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/formJualFragment">

    <fragment
        android:id="@+id/formJualFragment"
        android:name="id.finalproject.binar.secondhand.FormJualFragment"
        android:label="fragment_form_jual"
        tools:layout="@layout/fragment_form_jual" >
        <action
            android:id="@+id/action_formJualFragment_to_previewFragment"
            app:destination="@id/previewFragment" />
        <action
            android:id="@+id/action_formJualFragment_to_daftarJualFragment"
            app:destination="@id/daftarJualFragment" />
    </fragment>
    <fragment
        android:id="@+id/previewFragment"
        android:name="id.finalproject.binar.secondhand.PreviewFragment"
        android:label="fragment_preview"
        tools:layout="@layout/fragment_preview" >
        <action
            android:id="@+id/action_previewFragment_to_formJualFragment"
            app:destination="@id/formJualFragment" />
        <action
            android:id="@+id/action_previewFragment_to_daftarJualFragment"
            app:destination="@id/daftarJualFragment" />
    </fragment>
    <fragment
        android:id="@+id/daftarJualFragment"
        android:name="id.finalproject.binar.secondhand.DaftarJualFragment"
        android:label="fragment_daftar_jual"
        tools:layout="@layout/fragment_daftar_jual" />
</navigation>